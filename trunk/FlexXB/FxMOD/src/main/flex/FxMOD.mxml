	<fxmod:ApplicationController id="controller"/>
	<fxmod:DynamicMenuGenerator id="generator"/>
	<mx:Canvas width="100%" height="100%"/>
	<mx:Style source="/assets/css/FxMODStyle.css"/>
</mx:WindowedApplication>
