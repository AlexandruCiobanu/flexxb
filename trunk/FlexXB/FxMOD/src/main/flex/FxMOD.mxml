<mx:WindowedApplication xmlns:mx="http://www.adobe.com/2006/mxml" minHeight="600" minWidth="800"
		xmlns:fxmod="com.googlecode.fxmod.*"
		layout="absolute"
		addedToStage="{stage.nativeWindow.menu = generator.getMenu(controller)}"
		creationComplete="controller.initialize(this)" xmlns:text="flash.text.*">
